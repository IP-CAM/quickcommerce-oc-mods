<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\OpenCart\EbayShippingLocation" table="oc2_ebay_shipping_location">
    <id name="ebayShippingId" type="integer" column="ebay_shipping_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="detailVersion" type="string" column="detail_version" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="shippingLocation" type="string" column="shipping_location" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updateTime" type="string" column="update_time" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
