<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\OpenCart\AmazonusProductSearch" table="oc2_amazonus_product_search">
    <id name="productId" type="integer" column="product_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="status" type="string" column="status" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="matches" type="integer" column="matches" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="data" type="text" column="data" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
