<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\OpenCart\EbayPaymentMethod" table="oc2_ebay_payment_method">
    <id name="ebayPaymentMethodId" type="integer" column="ebay_payment_method_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ebayName" type="string" column="ebay_name" length="50" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="localName" type="string" column="local_name" length="50" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
