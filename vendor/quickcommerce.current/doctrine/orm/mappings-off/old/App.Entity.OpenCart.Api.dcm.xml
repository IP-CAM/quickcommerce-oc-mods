<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\OpenCart\Api" table="oc2_api">
    <id name="apiId" type="integer" column="api_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="text" column="password" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="dateAdded" type="datetime" column="date_added" nullable="false"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="false"/>
  </entity>
</doctrine-mapping>
